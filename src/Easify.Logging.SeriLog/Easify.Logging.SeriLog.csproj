<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Aurimas Gecas, Mohammad Moattar</Authors>
    <Company>ICG</Company>
    <Copyright>ICG</Copyright>
    <PackageProjectUrl>https://github.com/icgam/Easify</PackageProjectUrl>
    <RepositoryUrl>https://github.com/icgam/Easify</RepositoryUrl>
    <RepositoryType>Private</RepositoryType>
    <Description>Easify Logging support for Serilog</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Serilog" Version="2.6.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Easify.Logging\Easify.Logging.csproj" />
  </ItemGroup>

</Project>